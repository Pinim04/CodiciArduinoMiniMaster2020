int pinLed = 9;
int potLed = 0;
void setup() {
  // put your setup code here, to run once:
  pinMode(pinLed, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  potLed = 0;
  analogWrite(pinLed, potLed);
  delay(500);

  potLed = potLed + 50;
  analogWrite(pinLed, potLed);
  delay(500);

  potLed = potLed + 50;
  analogWrite(pinLed, potLed);
  delay(500);

  potLed = potLed + 50;
  analogWrite(pinLed, potLed);
  delay(500);

  potLed = potLed + 50;
  analogWrite(pinLed, potLed);
  delay(500);

  potLed = potLed + 55;
  analogWrite(pinLed, potLed);
  delay(500);

}
